{{ apnscp_root }}/storage/logs/bootstrapper.log {
	missingok
	dateext
	dateformat -%Y%m%d.log
	notifempty
	compress
	daily
	rotate 7
}

{{ apnscp_root }}/storage/logs/*_log 
	{{ apnscp_root }}/storage/logs/start.log {
	weekly
	missingok
	rotate 4
	compress
	lastaction
		systemctl restart apnscp
	endscript
}